+#include "kernel/param.h"
+#include "kernel/types.h"
+#include "kernel/stat.h"
+#include "user/user.h"
+
+int main(int argc, char *argv[])
+{
+    if (argc != 2)
+    {
+        printf("Usage: dummyproc <number of tickets>\n");
+        exit(1);
+    }
+    settickets(atoi(argv[1]));
+    int rc = fork();
+    if (rc < 0)
+    {
+        printf("Error forking\n");
+        exit(1);
+    }
+    else if (rc == 0)
+    {
+        // child process
+        while (1)
+        {
+            // running
+        }
+    }
+    exit(0);
+}
